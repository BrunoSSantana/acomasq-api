{
  "name": "acomasq-api",
  "version": "0.0.1",
  "description": "acomasq-api",
  "author": "BrunoSSantana",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prebuild": "rimraf dist && tsc -p tsconfig.build.json",
    "build:prod": "tsc -p tsconfig.build.json && nest build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run prisma:generate && npm run prisma:migration:push && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "prisma generate --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:migration": "prisma migrate dev --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:migration:push": "prisma migrate deploy --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/infra/repositories/prisma/schema.prisma",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.2",
    "@nestjs/common": "10.3.3",
    "@nestjs/config": "3.2.0",
    "@nestjs/core": "10.3.3",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.3",
    "@nestjs/swagger": "7.3.0",
    "@nestjs/terminus": "10.2.3",
    "@prisma/client": "5.10.2",
    "axios": "1.6.7",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "module-alias": "2.2.3",
    "passport": "0.7.0",
    "reflect-metadata": "0.2.1",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "zod": "3.22.4",
    "zod-validation-error": "3.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.3",
    "@swc/cli": "0.3.9",
    "@swc/core": "1.4.2",
    "@types/bcryptjs": "2.4.6",
    "@types/express": "4.17.21",
    "@types/node": "20.11.20",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "@vitest/coverage-istanbul": "1.3.1",
    "@vitest/ui": "1.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vitest": "0.3.22",
    "prettier": "3.2.5",
    "prisma": "5.10.2",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3",
    "unplugin-swc": "1.4.4",
    "vitest": "1.3.1"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}

{
  "name": "acomasq-api",
  "version": "0.0.1",
  "description": "acomasq-api",
  "author": "BrunoSSantana",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prebuild": "rimraf dist && tsc -p tsconfig.build.json",
    "build:prod": "tsc -p tsconfig.build.json && nest build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run prisma:migration:push && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "npx prisma generate --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:migration:create": "npx prisma migrate dev --schema=./src/infra/repositories/prisma/schema.prisma --name ",
    "prisma:migration:run": "npx prisma migrate dev --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:migration:push": "npx prisma migrate deploy --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./src/infra/repositories/prisma/schema.prisma",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "test:e2e": "vitest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/swagger": "6.2.1",
    "@prisma/client": "^4.10.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "module-alias": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@swc/core": "^1.3.35",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest": "^0.0.34",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}

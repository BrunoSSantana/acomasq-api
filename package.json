{
  "name": "acomasq-api",
  "version": "0.0.1",
  "description": "acomasq-api",
  "author": "BrunoSSantana",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prebuild": "rimraf dist && tsc -p tsconfig.build.json",
    "build:prod": "tsc -p tsconfig.build.json && nest build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run prisma:generate && npm run prisma:migration:push && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "npx prisma generate --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:migration": "npx prisma migrate dev --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:migration:push": "npx prisma migrate deploy --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./src/infra/repositories/prisma/schema.prisma",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "test:e2e": "vitest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "1.9.2",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/terminus": "9.2.2",
    "@prisma/client": "^4.13.0",
    "axios": "1.3.6",
    "class-transformer": "0.5.1",
    "module-alias": "^2.2.2",
    "openapi3-ts": "3.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@swc/core": "^1.3.55",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest": "^0.1.4",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.0.4",
    "vitest": "^0.30.1"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}

{
  "name": "acomasq-api",
  "version": "0.0.1",
  "description": "acomasq-api",
  "author": "BrunoSSantana",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prebuild": "rimraf dist && tsc -p tsconfig.build.json",
    "build:prod": "tsc -p tsconfig.build.json && nest build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run prisma:generate && npm run prisma:migration:push && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "npx prisma generate --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:migration": "npx prisma migrate dev --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:migration:push": "npx prisma migrate deploy --schema=./src/infra/repositories/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./src/infra/repositories/prisma/schema.prisma",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "test:e2e": "vitest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "2.0.3",
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.2.6",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.6",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "10.2.6",
    "@nestjs/swagger": "7.1.13",
    "@nestjs/terminus": "10.1.1",
    "@prisma/client": "5.4.1",
    "axios": "1.5.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "module-alias": "2.2.3",
    "openapi3-ts": "4.1.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.18",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.6",
    "@swc/core": "1.3.91",
    "@types/express": "4.17.18",
    "@types/node": "20.8.2",
    "@types/supertest": "2.0.14",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-vitest": "0.3.2",
    "prettier": "3.0.3",
    "prisma": "5.4.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}

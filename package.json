{
  "name": "acomasq-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist && tsc -p tsconfig.build.json",
    "build:prod": "nest build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run prisma:migration:push && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "prisma:migration:create": "npx prisma migrate dev --schema=./prisma/schema.prisma --name ",
    "prisma:migration:run": "npx prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:migration:push": "npx prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./prisma/schema.prisma",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/cli": "^9.1.2",
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.11",
    "@prisma/client": "^4.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.11",
    "@swc/core": "^1.3.0",
    "@swc/jest": "^0.2.22",
    "@types/express": "^4.17.13",
    "@types/jest": "29.0.1",
    "@types/node": "^18.7.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.8.3"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
